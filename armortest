local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerValues = LocalPlayer.PlayerValues
local Specialization = PlayerValues.Specialization
local Race = PlayerValues.Race

local S_time = 1.5
local S_time2 = 1
local S_time3 = 2.5
local MMS = "Madara's Mangekyo Sharingan v1"
local Itachi1 = "Itachi's Mangekyo Sharingan v1"
local Itachi2 = "Itachi's Mangekyo Sharingan v2"
_G.SSpin = true

local function autoarmor()
    local itemStorages = {"ItemStorage1", "ItemStorage2", "ItemStorage3", "ItemStorage4", "ItemStorage5", "ItemStorage6", "ItemStorage7"}
    for _, itemStorage in pairs(itemStorages) do
        local args = {itemStorage}
        game:GetService("ReplicatedStorage").RemoteEvents.ItemStorageRemote:FireServer(unpack(args))
        wait(S_time)
        LocalPlayer.Backpack:FindFirstChildOfClass("Tool").Parent = LocalPlayer.Character
        wait(S_time2)
        game:GetService("ReplicatedStorage").RemoteEvents.ItemRemote:FireServer()
        game:GetService("ReplicatedStorage").RemoteEvents.ItemStorageRemote:FireServer(unpack(args))
        wait(S_time2)
    end

    local args = {"Better Clothes"}
    game:GetService("ReplicatedStorage").RemoteEvents.ShenronRemote:FireServer(unpack(args))
    wait(S_time2)
    LocalPlayer.Backpack:FindFirstChildOfClass("Tool").Parent = LocalPlayer.Character
    wait(S_time2)
    local args = {"!giveitem LongHunterZ"}
    game:GetService("ReplicatedStorage").RemoteEvents.CommandsRemote:FireServer(unpack(args))
    wait(S_time3)
end

while _G.SSpin do
    autoarmor()
end
